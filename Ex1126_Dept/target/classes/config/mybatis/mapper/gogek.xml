<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="g">
	
	<!-- 고객 정보 조회 -->
	<select id="gogek_list"
			resultType="gogek"
			parameterType="int">
		select * from gogek 
		where godam = #{sabun}	
	</select>
	
</mapper>












